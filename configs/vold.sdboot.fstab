# internal sdcard
dev_mount sdcard /mnt/sdcard auto /devices/platform/asoc-mmc0.0/mmc_host/mmc0/*/block/mmcblk0/mmcblk0p12

# external sdcard
dev_mount sd-ext /mnt/sd-ext auto /devices/platform/asoc-mmc0.0/mmc_host/mmc0/*/block/mmcblk0/mmcblk0p13

# usbdisk
dev_mount uhost /storage/uhost auto /devices/platform/actions-dwc3.1/dwc3.0/xhci-hcd